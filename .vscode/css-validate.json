{
  "css.lint.unknownAtRules": "ignore",
  "css.lint.emptyRulesets": "ignore", 
  "css.lint.invalidProperties": "ignore",
  "postcss.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "tailwindCSS.validate": true,
  "tailwindCSS.lint.cssConflict": "warning",
